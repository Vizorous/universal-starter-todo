{
	"name": "frontend",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"predev": "relay-compiler --watch",
		"dev": "vite",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"test/**/*.ts\" \"test/**/*.tsx\"",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"relay": "relay-compiler --watch",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" \"{src,apps,libs,test}/**/*.tsx\" --fix"
	},
	"relay": {
		"src": "./src/",
		"schema": "./schema.graphql",
		"eagerEsModules": true,
		"language": "typescript",
		"artifactDirectory": "./src/__gen__/"
	},
	"dependencies": {
		"@babel/runtime": "^7.18.6",
		"@emotion/react": "^11.10.4",
		"@mantine/core": "^5.7.2",
		"@mantine/dates": "^4.2.12",
		"@mantine/dropzone": "^4.2.12",
		"@mantine/form": "^4.2.12",
		"@mantine/hooks": "^5.7.2",
		"@mantine/modals": "^4.2.12",
		"@mantine/next": "^4.2.12",
		"@mantine/notifications": "^4.2.12",
		"@seamlessc/tabler-icons-react": "^1.53.2",
		"case": "^1.6.3",
		"date-fns": "^2.28.0",
		"lodash.groupby": "^4.6.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-grid-layout": "^1.3.4",
		"react-relay": "^14.1.0",
		"relay-runtime": "^14.1.0"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.3.0",
		"@testing-library/react-hooks": "^8.0.1",
		"@testing-library/user-event": "^14.2.1",
		"@types/jsdom": "^16.2.14",
		"@types/lodash.groupby": "^4.6.7",
		"@types/node": "^18.0.0",
		"@types/react": "^18.0.14",
		"@types/react-dom": "^18.0.5",
		"@types/react-relay": "^14.1.2",
		"@types/relay-runtime": "^14.1.4",
		"@types/testing-library__jest-dom": "^5.14.5",
		"@types/ws": "^8.5.3",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"@vitejs/plugin-react": "^1.3.2",
		"@vitest/ui": "^0.16.0",
		"babel-plugin-relay": "^14.0.0",
		"eslint": "8.15.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-standard-with-typescript": "^23.0.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0",
		"eslint-plugin-only-warn": "^1.0.3",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-unicorn": "^42.0.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"graphql": "^16.5.0",
		"jsdom": "^20.0.0",
		"relay-compiler": "^14.1.0",
		"typescript": "*",
		"vite": "^2.9.13",
		"vitest": "^0.16.0"
	}
}

